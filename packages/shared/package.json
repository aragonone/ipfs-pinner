  "license": "GPL-3.0-or-later",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "lint": "eslint src/** --ext .ts",
    "build": "yarn lint && yarn build:types && yarn build:js",
    "build:types": "tsc",
    "build:js": "babel ./src --out-dir ./build --extensions .ts --root-mode upward",
